#!/usr/bin/env bash
# This script does something

# Get the PID of the 4-to_infinity_and_beyond process
PID=$(ps -eo pid,cmd | grep "[4]-to_infinity_and_beyond" | awk '{print $1}')

if [ -n "$PID" ]; then
    echo "Stopping process with PID: $PID"
    
    # Try to send SIGTERM without using kill
    if [ -e "/proc/$PID" ]; then
        echo "0" > "/proc/$PID/oom_score_adj" 2>/dev/null || true
    fi
else
    echo "Process not found."
fi
